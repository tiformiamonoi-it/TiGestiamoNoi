{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "npx dotenv-cli -e .env -- nodemon src/server.js",
    "postinstall": "prisma generate",
    "migrate": "npx dotenv-cli -e .env -- npx prisma migrate dev",
    "migrate:reset": "npx dotenv-cli -e .env -- npx prisma migrate reset",
    "db:push": "npx dotenv-cli -e .env -- npx prisma db push",
    "db:seed": "npx prisma db seed",
    "prisma:generate": "npx dotenv-cli -e .env -- npx prisma generate",
    "prisma:studio": "npx dotenv-cli -e .env -- npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "dotenv-cli": "^7.4.2",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "typescript": "^5.7.2"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}